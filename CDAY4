TO CALCULATE AMOUNT WITH RATE PERCENT:

#include <stdio.h>
#include<math.h>
int main() {
    float pa,fa;
    int years,rp;
    printf("Enter the principal amount :\n");
    scanf("%f",&pa);
    printf("Enter the rate power :\n");
    scanf("%d",&rp)
    printf("Enter the number of years:\n");
    scanf("%d",&years);
    fa = pa*

    return 0;
}




TO SWAP NUMBER USING POINTER:

#include<stdio.h>
int a,b;
void swap(int *a,int *b)
{
    int temp=*a;
    *a=*b;
    *b=temp;
}
int main()
{
    printf("Enter the value of a :\n");
    scanf("%d",&a);
    printf("Enter the value of b :\n");
    scanf("%d",&b);
    swap(&a,&b);
    printf("the value of a is : %d\n",a);
    printf("the value of b is : %d\n",b);
    return 0;
    
}




TO PRINT ADDRESS:

#include<stdio.h>
int main()
{
    int a[5]={1,2,3,4,5};
    for(int i=0;i<=5;i++)
    {
        printf("The address is : %p\n",&a[i]);
    }
}
